local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("[üéÉ] Slap Tower üñêÔ∏è", "Default")
local tab = DrRayLibrary.newTab("Main", "6319951718")
tab.newToggle("Autofarm", "The toggle off thing doesn't work you have to rejoin to stop the loop my bad I will try get it fixed next update", true, function(toggleState)
    if toggleState then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/6006-6006/Autofarm-Slap-Tower-/refs/heads/main/Main"))()
    else
        while true do
        print("Loop stopped")
        task.wait(math.huge)
        end
    end
end)
tab.newButton("Teleport to nearest candy", "broken will fix in next update my bad", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/6006-6006/Autofarm-Slap-Tower-/refs/heads/main/RitfLF0qB2"))()
end)
tab.newButton("Check if correct game", "If u don't get a notif ur in right game but if u get one our in wrong one", function()
    local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")

local correctPlaceId = 95508886069297

local function notif()
    StarterGui:SetCore("SendNotification", {
        Title = "Wrong Game!",
        Text = "You're not in the correct game, the correct game id is 95508886069297.",
        Duration = 5, -- seconds
    })
end

if game.PlaceId ~= correctPlaceId then
    notif()
end
end)
